<div class="col-md-12 column_div middle_div" style="height:400px;">
	<iframe src="http://mapunitygroups.com/community_map/solar-pumps?width=250&height=250&layers=solar-pump-list-2014-15" width=100% height=450 frameborder=0></iframe>
</div>
<div class="col-md-12 column_div middle_div" style="margin-top: 5rem;">
	<div class="data-box col-md-3">
		<span class="data-box-title"> NEWS </span>
		<ul class="data-box-contents">
			<% @feed_items.each do |item| %>
				<li class="data-box-contents-listings col-md-12">
					<div class="news">
						<%= image_tag "pen_news.png", class:"news-bullet" %>
						<a href='<%=item.link%>' class='news-item' target=_blank><%= item.title %></a>
					</div>
				</li>
			<% end %>
		</ul>
	</div>
	<div class="data-box col-md-3">
		<span class="data-box-title"> VIDEOS </span>
		<div class="data-box-contents">
		</div>
	</div>
	<div class="data-box col-md-3">
		<span class="data-box-title"> ORGANIZATION </span>
		<div class="data-box-contents">
			<a href='/dashboard'><%= image_tag "shakti-pumps.png" %></a>
		</div>
	</div>
	<div class="data-box col-md-3">
		<span class="data-box-title"> GALLERY </span>
		<div class="data-box-contents">
		</div>
	</div>
</div>
